{
  "name": "ecommerce",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:product-list": "npm run serve:single-spa:product-list",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:single-spa:product-list": "ng build product-list --prod",
    "serve:single-spa:product-list": "ng s --project product-list --disable-host-check --port 4400 --deploy-url http://localhost:4400/ --live-reload false",
    "build:single-spa:navbar": "ng build navbar --prod",
    "serve:single-spa:navbar": "ng s --project navbar --disable-host-check --port 4300 --deploy-url http://localhost:4300/ --live-reload false",
    "build:single-spa:shopping-cart": "ng build shopping-cart --prod",
    "serve:single-spa:shopping-cart": "ng s --project shopping-cart --disable-host-check --port 4100 --deploy-url http://localhost:4100/ --live-reload false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.0-",
    "@angular/common": "~12.1.0-",
    "@angular/compiler": "~12.1.0-",
    "@angular/core": "~12.1.0-",
    "@angular/elements": "~12.1.0-",
    "@angular/forms": "~12.1.0-",
    "@angular/platform-browser": "~12.1.0-",
    "@angular/platform-browser-dynamic": "~12.1.0-",
    "@angular/router": "~12.1.0-",
    "@webcomponents/custom-elements": "^1.5.0",
    "document-register-element": "^1.7.2",
    "rxjs": "~6.6.0",
    "single-spa": ">=4.0.0",
    "single-spa-angular": "5.0.2",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "12.1.3",
    "@angular-devkit/build-angular": "~12.1.4",
    "@angular/cli": "~12.1.3",
    "@angular/compiler-cli": "~12.1.0-",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ngx-build-plus": "^12.2.0",
    "ngx-fscaffold": "0.0.6",
    "typescript": "~4.3.2"
  }
}
